{
  "name": "ouyo",
  "description": "An Entity-Component-System library built for ease of use and code readability",
  "author": "Piotr Szlachciak <szlachciak.piotr@gmail.com>",
  "keywords": [
    "ecs",
    "entity",
    "component",
    "system"
  ],
  "license": "MIT",
  "version": "0.7.3",
  "main": "lib/ouyo.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sz-piotr/ouyo.git"
  },
  "scripts": {
    "start": "rollup -cw",
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": ""
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "rollup": "^0.58.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.14.0",
    "rollup-plugin-uglify": "^3.0.0",
    "temp-dir": "^1.0.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.(test|spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
